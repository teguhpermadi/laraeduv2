Grade::all()->mapWithKeys(function ($grade) {
                            return [$grade->id => $grade->name . ' (' . ($grade->is_inclusive ? 'inklusif' : 'ekslusif') . ')'];
                        })
Grade::all()->mapWithKeys(function ($grade) {
  return [$grade->id => $grade->name . ($grade->is_inclusive ? '(inklusif)' : '')];
})
Grade::all()->mapWithKeys(function ($grade) {
  return [$grade->id => $grade->name . ($grade->is_inclusive ? ' (inklusif)' : '')];
})
